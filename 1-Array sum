import java.util.*;
class task31
{
    public static void main(String[] args)
    {
        int a[]=new int[5];
         a[0]=10;
         a[1]=20;
         a[2]=30;
         a[3]=40;
         a[4]=50;
         int sum=0;
         Scanner Sc=new Scanner(System.in);
         for(int num:a)
         {
               sum=sum+num; 
         }
         double avg=(double)sum/a.length;
         System.out.println("sum"+sum);
         System.out.println("avg"+avg);
        }


    }

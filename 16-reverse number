import java.util.*;
class task16
{
    public static void main(String[] args)
    {
        int n,rev=0,rem;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the number");
        n=sc.nextInt();
        while(n!=0)
        {
            rem=n%10;
            rev=rev*10+rem;
            n=n/10;
        }
        System.out.println(rev);

    }
}
